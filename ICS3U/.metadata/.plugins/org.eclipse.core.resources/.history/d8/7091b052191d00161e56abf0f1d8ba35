import java.io.IOException;
import java.util.Scanner;

public class dmpg16b3 {

	public static void main(String[] args) throws IOException {
		Scanner blah=new Scanner (System.in);
		
		String designLine1=blah.nextLine();
		String designLine2=blah.nextLine();
		String designLine3=blah.nextLine();
		int v=blah.nextInt();
		int h=blah.nextInt();
		int w=blah.nextInt();
		
		for(int i=0;i!=v;i++)
		{
			System.out.print("=");
		}
		System.out.println("");
		System.out.println("|100");
		String [] spaces=new String[v-2];
		for(int i=0;i!=v-2;i++)
		{
			spaces[i]=" ";
		}
		
		for (int i=0;i!=h-1;i++)
		{
			System.out.print("|");
			for(int j=0;j!=v-2;j++)
			{
				System.out.print(spaces[j]);
			}
			System.out.print("|");
		}
	}

}
